{
  "manifest_version": 2,
  "name": "Bookmark posts on Google plus",
  "version": "2.0.0",
  "description": "Bookmark posts on Google plus",
  "permissions": [
    "notifications"
  ],
  "icons": {
    "48" : "48.png",
    "128" : "128.png"
  },
  "web_accessible_resources": [
    "48.png"
  ],
  "content_scripts": [
    {
      "matches": ["https://plus.google.com/*"],
      "js" : ["plus-bookmarker.js"]
    }
  ]
}
